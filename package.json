{
  "name": "drizzle-crud",
  "version": "0.1.4",
  "type": "module",
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "directory": "packages/drizzle-crud"
  },
  "scripts": {
    "build": "tsc --project tsconfig.json",
    "test": "bun test",
    "test:watch": "bun test --watch",
    "db:push": "drizzle-kit push --config=exemples/drizzle.config.ts",
    "db:pull": "drizzle-kit pull --config=exemples/drizzle.config.ts",
    "db:studio": "drizzle-kit studio --config=exemples/drizzle.config.ts",
    "db:generate": "drizzle-kit generate --config=exemples/drizzle.config.ts",
    "db:migrate": "drizzle-kit migrate --config=exemples/drizzle.config.ts"
  },
  "exports": {
    ".": {
      "sjs": "./src/index.ts",
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    },
    "./zod": {
      "sjs": "./src/zod.ts",
      "types": "./dist/zod.d.ts",
      "import": "./dist/zod.js"
    }
  },
  "peerDependencies": {
    "drizzle-orm": "^0.44.2",
    "drizzle-zod": "^0.8.2",
    "zod": "^4.1.5"
  },
  "devDependencies": {
    "@biomejs/biome": "^2.2.2",
    "@libsql/client": "^0.15.14",
    "@types/bun": "latest",
    "drizzle-orm": "^0.44.2",
    "drizzle-zod": "^0.8.2",
    "typescript": "^5.7.3",
    "zod": "^4.1.5"
  }
}